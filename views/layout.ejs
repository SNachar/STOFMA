  <body ng-controller="MainCtrl as main">
    <div class="screen-intro" layout="column" layout-align="center center" ng-hide="appLoaded">
      <h1 class="md-display-1 ng-cloak">Student Office Manager</h1>

      <h2 hide-sm class="md-title ng-cloak">Faites affaire sereinement.</h2>
      <img src="/images/logo_stofma.png" alt="Logo for intro" class="text-center ng-cloak">
      <md-progress-circular class="md-accent" md-diameter="32" md-mode="indeterminate"></md-progress-circular>
    </div>
    <div class="wrap" layout="row" ng-cloak>
      <%- body %>
    </div>

    <script> /* To prevent error on underscore */
      var _ = {escape: null}; </script>
    <!--TEMPLATES-->
    <script type="text/javascript" src="/jst.js"></script>
    <!--TEMPLATES END-->


    <!--SCRIPTS-->
